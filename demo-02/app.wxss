.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix{
  width: 95%;
  padding:20rpx;
  overflow: hidden;
  height:auto;
}
.clearfix image{
    width: 70rpx;
    height: 70rpx;
    margin-top: 16rpx;
}